# Placeholder for lazy devs. Builds debug version of Mk4.
#
# Normally you should use:
#
#	make -f MK4-Makefile
# or
#	make -f MK3-Makefile
#

.DEFAULT all:
	$(MAKE) DEBUG_BUILD=1 -f MK4-Makefile


release:
	$(MAKE) -f MK4-Makefile release
	$(MAKE) -f MK3-Makefile release

